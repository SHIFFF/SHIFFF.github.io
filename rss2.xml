      <category domain="http://shiff.top/tags/idea/">idea</category>
      
      <category domain="http://shiff.top/tags/springboot/">springboot</category>
      
      <category domain="http://shiff.top/tags/jdk/">jdk</category>
      
      
      <comments>http://shiff.top/2023/03/11/%E6%97%A0%E6%95%88%E7%9A%84%E7%9B%AE%E6%A0%87%E5%8F%91%E8%A1%8C%E7%89%8817/#disqus_thread</comments>
      
    </item>
    
